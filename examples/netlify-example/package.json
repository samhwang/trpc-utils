{
  "name": "netlify-example",
  "version": "1.0.0",
  "private": true,
  "description": "Example for App with Netlify Functions",
  "scripts": {
    "ci": "rome ci ./netlify ./trpc",
    "lint": "rome check ./netlify ./trpc",
    "lint:fix": "rome check --apply ./netlify ./trpc",
    "lint:fix:unsafe": "rome check --apply-unsafe ./netlify ./trpc",
    "format": "rome format --write ./netlify ./trpc",
    "typecheck": "tsc",
    "dev": "netlify functions:serve"
  },
  "dependencies": {
    "@netlify/functions": "^1.6.0",
    "@trpc/server": "^10.37.1",
    "trpc-netlify-functions": "*",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "netlify-cli": "^15.10.0"
  }
}
